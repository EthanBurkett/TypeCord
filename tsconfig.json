{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "$/*": [
        "./$modules/*"
      ],
      "#/*": [
        "./types/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
